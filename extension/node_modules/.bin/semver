#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/abuaa/Projects/allowance-guard/extension/node_modules/semver/bin/node_modules:/Users/abuaa/Projects/allowance-guard/extension/node_modules/semver/node_modules:/Users/abuaa/Projects/allowance-guard/extension/node_modules:/Users/abuaa/Projects/allowance-guard/node_modules:/Users/abuaa/Projects/node_modules:/Users/abuaa/node_modules:/Users/node_modules:/node_modules:/Users/abuaa/Projects/allowance-guard/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/abuaa/Projects/allowance-guard/extension/node_modules/semver/bin/node_modules:/Users/abuaa/Projects/allowance-guard/extension/node_modules/semver/node_modules:/Users/abuaa/Projects/allowance-guard/extension/node_modules:/Users/abuaa/Projects/allowance-guard/node_modules:/Users/abuaa/Projects/node_modules:/Users/abuaa/node_modules:/Users/node_modules:/node_modules:/Users/abuaa/Projects/allowance-guard/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../semver/bin/semver.js" "$@"
else
  exec node  "$basedir/../semver/bin/semver.js" "$@"
fi
